mixin blogPosts (item)
    .blog-img
        img(src=item.src, alt=item.alt)
    .post-info 
        .data-blog-post.flex 
            .data-post 
                p #{item.dataPost}
                span #{item.monthPost}
            .name-post 
                p.text-left #{item.namePost}
                i.text-left.sub-name-post By 
                    span Khalil 
                    | Uddin in 
                    span Development!
        p.text-info #{item.description}
        a.button-more(href="#") Read more
